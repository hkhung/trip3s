			};

			javo_write_review.prototype = {

				constructor : javo_write_review

				, init : function()
				{
					var el				= this.el;
					var form			= el.find( "form" );
					this.list_offset	= 0;

					form.removeClass( 'hidden' ).hide();

					this
						.setInputRatingFields();

					$( document )

						.on( 'click', this.selector, this.showWriteForm )
