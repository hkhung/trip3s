				if (options.items.length < 0) {
					return;
				};
				var item_markers	= new Array(),response = [],obj = $(this);

				$.post('/api/detail_by_schedule_id',{schedule_id: $(this).data('scheduleid')}).done(function(data){
					response = data;
					
					$.each( response, function( i, item ){
