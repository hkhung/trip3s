				, setInputRatingFields : function( reset )
				{
					var element		= $('.javo_rat_star');

					element.each(
						function(k, v)
						{
							if( ! reset )
							{
								$( this ).raty({
									starOff: '/assets/star-off.png'
									, starOn: '/assets/star-on.png'
									, starHalf: '/assets/star-half.png'
									, half: true
									, width:150
