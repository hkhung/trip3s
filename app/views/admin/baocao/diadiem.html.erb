<section id="main-content">
<div class="row">
                  <div class="col-lg-12">
                      <section class="panel">
                          <header class="panel-heading">
                              <h2>Danh Sách Top Địa Điểm</h2>
                             
                          </header>
                          
                          <table class="table table-striped table-advance table-hover">
                           <tbody>
                              <tr>
                                 <th><i class="icon_profile"></i> Tên Địa Điểm</th>
                                 <th><i class="icon_profile"></i> Địa Chỉ</th>
                                 <th><i class="icon_profile"></i> Điểm </th>
                                 <th><i class="icon_profile"></i> Review </th>
                              </tr>
                              <% @places_top.each do |place| %>
                              <tr>
                                  <td><%= place.post.post_title %></td>
                                  <td><%= place.place_address %></td>
                                  <td><%= place.place_choice %></td>
                                  <td><%= place.post.post_review %></td>
                                </tr>
                              <% end %>
                           </tbody>
                        </table>
                        <%= will_paginate @places_top %>
                      </section>
 
<br>
</section>