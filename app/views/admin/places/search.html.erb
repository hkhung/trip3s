<section id="main-content">
<div class="row">
                  <div class="col-lg-12">
                      <section class="panel">
                          <header class="panel-heading">
                              <h2>Danh Sách Người Dùng Mới</h2>
                            
                          </header>
                        
                          <table class="table table-striped table-advance table-hover">
                           <tbody>
                              <tr>
                                 <th><i class="icon_profile"></i> Tên Địa Điểm</th>
                                 <th><i class="icon_calendar"></i> Địa Chỉ</th>
                                 <th><i class="icon_cogs"></i> Hành Động</th>
                              </tr>
                            <% for place in @places_search %>
                              <tr>
                                  <td><%= place.post.post_title %></td>
                                  <td><%= place.place_address %></td>
                                   <td>
                                  <div class="btn-group">
                                      <%= link_to 'Xem', [:admin, place] %>
                                      ||<%= link_to 'Sửa', edit_admin_place_path(place) %>
                                      ||<%= link_to 'Xóa', [:admin, place], method: :delete, data: { confirm: 'Are you sure?' } %>
                                  </div>
                                  </td>
                                </tr>
                              <% end %>
                           </tbody>
                        </table>
                        <%= will_paginate @places_search %>
                      </section>
 
<br>
</section>